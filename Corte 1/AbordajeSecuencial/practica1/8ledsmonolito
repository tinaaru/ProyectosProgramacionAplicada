from machine import Pin
import time

pin2=Pin(2, Pin.OUT)
pin5=Pin(5, Pin.OUT)
pin18=Pin(18, Pin.OUT)
pin19=Pin(19, Pin.OUT)
pin21=Pin(21, Pin.OUT)
pin22=Pin(22, Pin.OUT)
pin23=Pin(23, Pin.OUT)
pin25=Pin(25, Pin.OUT)

while True:
    # Para el LED del pin 2
    pin2.value(1)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    # Para el LED del pin 5   
    pin2.value(0)
    pin5.value(1)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    # Para el LED del pin 18
    pin2.value(0)
    pin5.value(0)
    pin18.value(1)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    # Para el LED del pin 19
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(1)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    # Para el LED del pin 21
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(1)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    # Para el LED del pin 22
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(1)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
    # Para el LED del pin 23
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(1)
    pin25.value(0)
    time.sleep(1)
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)

    # Para el LED del pin 25
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(1)
    time.sleep(1)
    pin2.value(0)
    pin5.value(0)
    pin18.value(0)
    pin19.value(0)
    pin21.value(0)
    pin22.value(0)
    pin23.value(0)
    pin25.value(0)
    time.sleep(1)
